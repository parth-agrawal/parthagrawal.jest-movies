{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "loadenv": "npx dotenv-cli -e .env.test",
    "test:dockup": "docker compose -f docker-compose.test.yml up -d",
    "test:migrate": "npx prisma migrate dev",
    "test:run": "bun vitest",
    "test:dockdown": "docker compose -f docker-compose.test.yml down",
    "test": "npx dotenv-cli -e .env.test -- npm run test:dockup && npx dotenv-cli -e .env.test -- npm run test:migrate && npx dotenv-cli -e .env.test -- npm run test:run && npx dotenv-cli -e .env.test -- npm run test:dockdown",
    "dev": "npx dotenv-cli -e .env.dev -- vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "@types/cors": "^2.8.17",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "prisma": "^5.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  }
}